#xp.button 
	Level XP: 
	=@user.total_xp%1000
#show_user_checkins.button Checkins
#show_world_checkins.button World Checkins
#show_all_quests.button All Quests
#show_available_quests.button Available Quests
#show_completed_quests.button Completed Quests
#show_created_quests.button Created Quests
%br
%br
#show_active_quests.button Active Quests
#show_user_quests
	%ul
		-@user.quests.each do |quest|
			-if quest.creator_id != @user.id
				%div{id: "#{quest.id}", class: "button quest"}
					%li
						=quest.title
